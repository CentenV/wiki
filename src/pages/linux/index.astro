---
import Banner from "@assets/linux.jpg";
import IntermediatePage from "@components/IntermediatePage.astro";
const title: string = "Linux";
const url: string = Astro.url.pathname;
const pages = import.meta.glob("./*mdx", { eager: true });
---

<IntermediatePage
    title={title}
    url={url}
    pages={pages}
    banner={Banner}
    bannerOptions={{ offset: "-translate-y-32" }}
/>
